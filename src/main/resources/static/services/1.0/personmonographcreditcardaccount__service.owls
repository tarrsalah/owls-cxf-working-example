</process:Input>

<process:Input rdf:ID="_PERSON">
<process:parameterType rdf:resource="http://127.0.0.1/ontology/books.owl#Person" />
  <rdfs:label></rdfs:label>
</process:Input>

<grounding:WsdlGrounding rdf:ID="PERSONMONOGRAPHCREDITCARDACCOUNT__GROUNDING">
