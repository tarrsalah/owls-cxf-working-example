<?xml version="1.0"?>
<rdf:RDF
    xmlns:process="http://www.daml.org/services/owl-s/1.1/Process.owl#"
    xmlns:flight-company="http://127.0.0.1/ontology/messemodul.owl#"
    xmlns:list="http://www.daml.org/services/owl-s/1.1/generic/ObjectList.owl#"
    xmlns:swrl="http://www.w3.org/2003/11/swrl#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:expr="http://www.daml.org/services/owl-s/1.1/generic/Expression.owl#"
    xmlns:swrlb="http://www.w3.org/2003/11/swrlb#"
    xmlns="http://127.0.0.1/services/1.1/close_door.owls#"
    xmlns:service="http://www.daml.org/services/owl-s/1.1/Service.owl#"
    xmlns:grounding="http://www.daml.org/services/owl-s/1.1/Grounding.owl#"
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
    xmlns:daml="http://www.daml.org/2001/03/daml+oil#"
    xmlns:dc="http://purl.org/dc/elements/1.1/"
    xmlns:profile="http://www.daml.org/services/owl-s/1.1/Profile.owl#"
  xml:base="http://127.0.0.1/services/1.1/close_door.owls">
  <owl:Ontology rdf:about="">
    <owl:imports rdf:resource="http://127.0.0.1/ontology/Service.owl" />
	<owl:imports rdf:resource="http://127.0.0.1/ontology/Process.owl" />
	<owl:imports rdf:resource="http://127.0.0.1/ontology/Profile.owl" />
	<owl:imports rdf:resource="http://127.0.0.1/ontology/Grounding.owl" />
	<owl:imports rdf:resource="http://127.0.0.1/ontology/Expression.owl" />
    <owl:imports rdf:resource="http://127.0.0.1/ontology/messemodul.owl"/>
  </owl:Ontology>
  <profile:Profile rdf:ID="CloseDoorProfile">
    <profile:hasInput>
      <process:Input rdf:ID="Door">
        <process:parameterType rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI"
        >http://127.0.0.1/ontology/messemodul.owl#Door</process:parameterType>
      </process:Input>
    </profile:hasInput>
    <profile:serviceName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Close door</profile:serviceName>
    <profile:textDescription rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Closes the given open and unlocked door of a room.</profile:textDescription>
    <profile:has_process>
	<process:AtomicProcess rdf:ID="CloseDoorAtomicProcess">
		<process:hasPrecondition>
			<expr:SWRL-Condition rdf:ID="OpenAndUnlockedDoor">
				<expr:expressionLanguage rdf:resource="http://www.daml.org/services/owl-s/1.1/generic/Expression.owl#SWRL"/>
				<expr:expressionBody rdf:parseType="Literal">
					<swrl:AtomList>
						<rdf:first>
							<swrl:ClassAtom>
								<swrl:classPredicate rdf:resource="http://127.0.0.1/ontology/messemodul.owl#Open"/>
								<swrl:argument1 rdf:resource="#Door"/>
							</swrl:ClassAtom>
						</rdf:first>
						<rdf:rest>
							<swrl:AtomList>
								<rdf:first>
									<swrl:ClassAtom>
										<swrl:classPredicate rdf:resource="http://127.0.0.1/ontology/messemodul.owl#Unlocked"/>
										<swrl:argument1 rdf:resource="#Door"/>
									</swrl:ClassAtom>
								</rdf:first>
								<rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
							</swrl:AtomList>
						</rdf:rest>
					</swrl:AtomList>
				</expr:expressionBody>
			</expr:SWRL-Condition>
		</process:hasPrecondition>
        <process:hasInput rdf:resource="#Door"/>
        <service:describes>
          <service:Service rdf:ID="CloseDoorService">
            <service:describedBy rdf:resource="#CloseDoorAtomicProcess"/>
            <service:supports>
              <grounding:WsdlGrounding rdf:ID="CloseDoorWsdlGrounding">
                <service:supportedBy rdf:resource="#CloseDoorService"/>
				<grounding:hasAtomicProcessGrounding>
					<grounding:WsdlAtomicProcessGrounding rdf:ID="CloseDoorAtomicProcessGrounding"/>
				</grounding:hasAtomicProcessGrounding>
              </grounding:WsdlGrounding>
            </service:supports>
            <service:presents rdf:resource="#CloseDoorProfile"/>
          </service:Service>
        </service:describes>
        <process:hasResult>
		  <process:Result rdf:ID="ClosedDoor">
			<process:hasEffect>
				<expr:SWRL-Expression>
					<expr:expressionLanguage rdf:resource="http://www.daml.org/services/owl-s/1.1/generic/Expression.owl#SWRL"/>
					<expr:expressionBody rdf:parseType="Literal">
						<swrl:AtomList>
							<rdf:first>
								<swrl:ClassAtom>
									<swrl:classPredicate rdf:resource="http://127.0.0.1/ontology/messemodul.owl#Closed"/>
									<swrl:argument1 rdf:resource="#Door"/>
								</swrl:ClassAtom>
							</rdf:first>
							<rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
						</swrl:AtomList>
					</expr:expressionBody>
				</expr:SWRL-Expression>
			</process:hasEffect>
		  </process:Result>
		</process:hasResult>
      </process:AtomicProcess>
    </profile:has_process>
    <service:presentedBy rdf:resource="#CloseDoorService"/>
  </profile:Profile>
  <grounding:WsdlAtomicProcessGrounding rdf:about="#CloseDoorAtomicProcessGrounding">
    <grounding:wsdlDocument rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI"
    >http://127.0.0.1/wsdl/close_door.wsdl</grounding:wsdlDocument>
    <grounding:owlsProcess rdf:resource="#CloseDoorAtomicProcess"/>
    <grounding:wsdlOperation>
      <grounding:WsdlOperationRef>
        <grounding:operation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI"
        >http://127.0.0.1/wsdl/Closedoor#get</grounding:operation>
        <grounding:portType rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI"
        >http://127.0.0.1/wsdl/Closedoor#ClosedoorSoap</grounding:portType>
      </grounding:WsdlOperationRef>
    </grounding:wsdlOperation>
    <grounding:wsdlInputMessage rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI"
    >http://127.0.0.1/wsdl/Closedoor#getRequest</grounding:wsdlInputMessage>
    <grounding:wsdlOutputMessage rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI"
    >http://127.0.0.1/wsdl/Closedoor#getResponse</grounding:wsdlOutputMessage>
    <grounding:wsdlInput>
      <grounding:WsdlInputMessageMap>
        <grounding:owlsParameter rdf:resource="#Door"/>
        <grounding:wsdlMessagePart rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI"
        >http://127.0.0.1/wsdl/Closedoor#Door</grounding:wsdlMessagePart>
        <grounding:xsltTransformationString>None (XSL)</grounding:xsltTransformationString>
      </grounding:WsdlInputMessageMap>
    </grounding:wsdlInput>
  </grounding:WsdlAtomicProcessGrounding>
</rdf:RDF>

