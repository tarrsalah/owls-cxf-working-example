    >http://127.0.0.1/wsdl/SFNRecommendedPrice_service.wsdl</grounding:wsdlDocument>
    <grounding:owlsProcess rdf:resource="#USERSCIENCE-FICTION-NOVEL_RECOMMENDEDPRICE_PROCESS"/>
