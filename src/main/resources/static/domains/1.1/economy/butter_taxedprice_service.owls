<?xml version="1.0" encoding="WINDOWS-1252"?>
<rdf:RDF  xmlns:owl       = "http://www.w3.org/2002/07/owl#"
xmlns:rdfs      = "http://www.w3.org/2000/01/rdf-schema#"
xmlns:rdf       = "http://www.w3.org/1999/02/22-rdf-syntax-ns#"
xmlns:service   = "http://www.daml.org/services/owl-s/1.1/Service.owl#"
xmlns:process   = "http://www.daml.org/services/owl-s/1.1/Process.owl#"
xmlns:profile    = "http://www.daml.org/services/owl-s/1.1/Profile.owl#"
xmlns:grounding = "http://www.daml.org/services/owl-s/1.1/Grounding.owl#"

xml:base        = "http://127.0.0.1/services/1.1/butter_taxedprice_service.owls">

<owl:Ontology rdf:about="">
<owl:imports rdf:resource="http://127.0.0.1/ontology/Service.owl" />
<owl:imports rdf:resource="http://127.0.0.1/ontology/Process.owl" />
<owl:imports rdf:resource="http://127.0.0.1/ontology/Profile.owl" />
<owl:imports rdf:resource="http://127.0.0.1/ontology/Grounding.owl" />
<owl:imports rdf:resource="http://127.0.0.1/ontology/Mid-level-ontology.owl" />
<owl:imports rdf:resource="http://127.0.0.1/ontology/concept.owl" />
</owl:Ontology>

<service:Service rdf:ID="BUTTER_TAXEDPRICE_SERVICE">
<service:presents rdf:resource="#BUTTER_TAXEDPRICE_PROFILE"/>
<service:describedBy rdf:resource="#BUTTER_TAXEDPRICE_PROCESS"/>
<service:supports rdf:resource="#BUTTER_TAXEDPRICE_GROUNDING"/>
</service:Service>

<profile:Profile rdf:ID="BUTTER_TAXEDPRICE_PROFILE">
<service:isPresentedBy rdf:resource="#BUTTER_TAXEDPRICE_SERVICE"/>
<profile:serviceName xml:lang="en">
Butter TaxedPrice Service
</profile:serviceName>
<profile:textDescription xml:lang="en">
This service returns taxed price of some butter brand. The price is for 1kg of butter.
</profile:textDescription>
<profile:hasInput  rdf:resource="#_BUTTER"/>
<profile:hasOutput rdf:resource="#_TAXEDPRICE"/>


<profile:has_process rdf:resource="BUTTER_TAXEDPRICE_PROCESS" /></profile:Profile>

<!--<process:ProcessModel rdf:ID="BUTTER_TAXEDPRICE_PROCESS_MODEL">
<service:describes rdf:resource="#BUTTER_TAXEDPRICE_SERVICE"/>
<process:hasProcess rdf:resource="#BUTTER_TAXEDPRICE_PROCESS"/>
</process:ProcessModel>-->

<process:AtomicProcess rdf:ID="BUTTER_TAXEDPRICE_PROCESS">
<service:describes rdf:resource="#BUTTER_TAXEDPRICE_SERVICE"/>
<process:hasInput  rdf:resource="#_BUTTER"/>
<process:hasOutput rdf:resource="#_TAXEDPRICE"/>
</process:AtomicProcess>

<process:Input rdf:ID="_BUTTER">
<process:parameterType rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">http://127.0.0.1/ontology/Mid-level-ontology.owl#Butter</process:parameterType>
<rdfs:label></rdfs:label>
</process:Input>

<process:Output  rdf:ID="_TAXEDPRICE">
<process:parameterType rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">http://127.0.0.1/ontology/concept.owl#TaxedPrice</process:parameterType>
<rdfs:label></rdfs:label>
</process:Output>


<grounding:WsdlGrounding rdf:ID="BUTTER_TAXEDPRICE_GROUNDING">
<service:supportedBy rdf:resource="#BUTTER_TAXEDPRICE_SERVICE"/>
<grounding:hasAtomicProcessGrounding>
      <grounding:WsdlAtomicProcessGrounding rdf:ID="BUTTER_TAXEDPRICE_AtomicProcessGrounding"/>
    </grounding:hasAtomicProcessGrounding>
</grounding:WsdlGrounding>

<grounding:WsdlAtomicProcessGrounding rdf:about="#BUTTER_TAXEDPRICE_AtomicProcessGrounding">
    <grounding:wsdlDocument rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI"
    >http://127.0.0.1/wsdl/ButterTaxedprice.wsdl</grounding:wsdlDocument>
    <grounding:owlsProcess rdf:resource="#BUTTER_TAXEDPRICE_PROCESS"/>
    <grounding:wsdlOperation>
      <grounding:WsdlOperationRef>
        <grounding:operation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI"
        >http://127.0.0.1/wsdl/ButterTaxedprice#get_TAXEDPRICE</grounding:operation>
        <grounding:portType rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI"
        >http://127.0.0.1/wsdl/ButterTaxedprice#ButterTaxedpriceSoap</grounding:portType>
      </grounding:WsdlOperationRef>
    </grounding:wsdlOperation>
    <grounding:wsdlInputMessage rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI"
    >http://127.0.0.1/wsdl/ButterTaxedprice#get_TAXEDPRICERequest</grounding:wsdlInputMessage>
    <grounding:wsdlOutputMessage rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI"
    >http://127.0.0.1/wsdl/ButterTaxedprice#get_TAXEDPRICEResponse</grounding:wsdlOutputMessage>
    <grounding:wsdlInput>
      <grounding:WsdlInputMessageMap>
        <grounding:owlsParameter rdf:resource="#_BUTTER"/>
        <grounding:wsdlMessagePart rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI"
        >http://127.0.0.1/wsdl/ButterTaxedprice#_BUTTER</grounding:wsdlMessagePart>
        <grounding:xsltTransformationString>None (XSL)</grounding:xsltTransformationString>
      </grounding:WsdlInputMessageMap>
    </grounding:wsdlInput>
    <grounding:wsdlOutput>
      <grounding:WsdlOutputMessageMap>
        <grounding:owlsParameter rdf:resource="#_TAXEDPRICE"/>
        <grounding:wsdlMessagePart rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI"
        >http://127.0.0.1/wsdl/ButterTaxedprice#_TAXEDPRICE</grounding:wsdlMessagePart>
        <grounding:xsltTransformationString>None (XSL)</grounding:xsltTransformationString>
      </grounding:WsdlOutputMessageMap>
    </grounding:wsdlOutput>
  </grounding:WsdlAtomicProcessGrounding>

</rdf:RDF>
