<?xml version="1.0" encoding="WINDOWS-1252"?>
<rdf:RDF  xmlns:owl       = "http://www.w3.org/2002/07/owl#"
xmlns:rdfs      = "http://www.w3.org/2000/01/rdf-schema#"
xmlns:rdf       = "http://www.w3.org/1999/02/22-rdf-syntax-ns#"
xmlns:service   = "http://www.daml.org/services/owl-s/1.1/Service.owl#"
xmlns:process   = "http://www.daml.org/services/owl-s/1.1/Process.owl#"
xmlns:profile    = "http://www.daml.org/services/owl-s/1.1/Profile.owl#"
xmlns:grounding = "http://www.daml.org/services/owl-s/1.1/Grounding.owl#"
xmlns:expr      = "http://www.daml.org/services/owl-s/1.1/generic/Expression.owl#"
xmlns:swrl		="http://www.w3.org/2003/11/swrl#"

xml:base        = "http://127.0.0.1/services/1.1/calculateDistanceInMiles.owls">

<owl:Ontology rdf:about="">
<owl:imports rdf:resource="http://127.0.0.1/ontology/Service.owl" />
<owl:imports rdf:resource="http://127.0.0.1/ontology/Process.owl" />
<owl:imports rdf:resource="http://127.0.0.1/ontology/Profile.owl" />
<owl:imports rdf:resource="http://127.0.0.1/ontology/Grounding.owl" />
<owl:imports rdf:resource="http://127.0.0.1/ontology/Expression.owl" />
<owl:imports rdf:resource="http://127.0.0.1/ontology/geographydataset.owl" />
<owl:imports rdf:resource="http://127.0.0.1/ontology/SUMO.owl" />
<owl:imports rdf:resource="http://127.0.0.1/ontology/protont.owl" />
</owl:Ontology>

<service:Service rdf:ID="CALCULATE_DISTANCE_MILES_SERVICE">
<service:presents rdf:resource="#CALCULATE_DISTANCE_MILES_PROFILE"/>
<service:describedBy rdf:resource="#CALCULATE_DISTANCE_MILES_PROCESS"/>
<service:supports rdf:resource="#CALCULATE_DISTANCE_MILES_GROUNDING"/>
</service:Service>

<profile:Profile rdf:ID="CALCULATE_DISTANCE_MILES_PROFILE">
<service:isPresentedBy rdf:resource="#CALCULATE_DISTANCE_MILES_SERVICE"/>
<profile:serviceName xml:lang="en">
Distance calculator between two locations
</profile:serviceName>
<profile:textDescription xml:lang="en">
This service computes the distance in miles between two given coordinates. The distance is a straight line.
</profile:textDescription>
<profile:hasInput  rdf:resource="#_LATITUDE1"/>
<profile:hasInput  rdf:resource="#_LONGITUDE1"/>
<profile:hasInput  rdf:resource="#_LATITUDE2"/>
<profile:hasInput  rdf:resource="#_LONGITUDE2"/>
<profile:hasOutput rdf:resource="#_DISTANCE"/>
<profile:hasPrecondition rdf:resource="#DifferentCoordinates"/>

<profile:has_process rdf:resource="CALCULATE_DISTANCE_MILES_PROCESS" /></profile:Profile>

<process:AtomicProcess rdf:ID="CALCULATE_DISTANCE_MILES_PROCESS">
<service:describes rdf:resource="#CALCULATE_DISTANCE_MILES_SERVICE"/>
<process:hasInput  rdf:resource="#_LATITUDE1"/>
<process:hasInput  rdf:resource="#_LONGITUDE1"/>
<process:hasInput  rdf:resource="#_LATITUDE2"/>
<process:hasInput  rdf:resource="#_LONGITUDE2"/>
<process:hasOutput rdf:resource="#_DISTANCE"/>
<process:hasPrecondition>
	<expr:SWRL-Condition rdf:ID="DifferentCoordinates">
		<expr:expressionLanguage rdf:resource="http://www.daml.org/services/owl-s/1.1/generic/Expression.owl#SWRL"/>
		<expr:expressionBody rdf:parseType="Literal">
			<swrl:AtomList>
				<rdf:first>
					<swrl:DifferentIndividualsAtom>
						<swrl:argument1 rdf:resource="#_LATITUDE1"/>
						<swrl:argument2 rdf:resource="#_LATITUDE2"/>
					</swrl:DifferentIndividualsAtom>
				</rdf:first>
				<rdf:rest>
					<swrl:AtomList>
						<rdf:first>
							<swrl:DifferentIndividualsAtom>
								<swrl:argument1 rdf:resource="#_LONGITUDE1"/>
								<swrl:argument2 rdf:resource="#_LONGITUDE2"/>
							</swrl:DifferentIndividualsAtom>
						</rdf:first>
						<rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
					</swrl:AtomList>
				</rdf:rest>
			</swrl:AtomList>
		</expr:expressionBody>
	</expr:SWRL-Condition>
</process:hasPrecondition>
</process:AtomicProcess>

<process:Input rdf:ID="_LATITUDE1">
<process:parameterType rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">http://127.0.0.1/ontology/protont.owl#latitude</process:parameterType>
<rdfs:label></rdfs:label>
</process:Input>

<process:Input rdf:ID="_LONGITUDE1">
<process:parameterType rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">http://127.0.0.1/ontology/protont.owl#longitude</process:parameterType>
<rdfs:label></rdfs:label>
</process:Input>

<process:Input rdf:ID="_LATITUDE2">
<process:parameterType rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">http://127.0.0.1/ontology/protont.owl#latitude</process:parameterType>
<rdfs:label></rdfs:label>
</process:Input>

<process:Input rdf:ID="_LONGITUDE2">
<process:parameterType rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">http://127.0.0.1/ontology/protont.owl#longitude</process:parameterType>
<rdfs:label></rdfs:label>
</process:Input>

<process:Output  rdf:ID="_DISTANCE">
<process:parameterType rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">http://127.0.0.1/ontology/geographydataset.owl#GeographicDistance</process:parameterType>
<rdfs:label></rdfs:label>
</process:Output>

<grounding:WsdlGrounding rdf:ID="CALCULATE_DISTANCE_MILES_GROUNDING">
  <service:supportedBy rdf:resource="#CALCULATE_DISTANCE_MILES_SERVICE"/>
    <grounding:hasAtomicProcessGrounding>
      <grounding:WsdlAtomicProcessGrounding rdf:ID="CALCULATE_DISTANCE_MILES_AtomicProcessGrounding"/>
    </grounding:hasAtomicProcessGrounding>
</grounding:WsdlGrounding>

<grounding:WsdlAtomicProcessGrounding rdf:about="#CALCULATE_DISTANCE_MILES_AtomicProcessGrounding">
    <grounding:wsdlInput>
      <grounding:WsdlInputMessageMap>
        <grounding:owlsParameter rdf:resource="#_LONGITUDE1"/>
        <grounding:wsdlMessagePart rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI"
        >http://127.0.0.1/wsdl/CalculateDistanceMiles#_LONGITUDE1</grounding:wsdlMessagePart>
        <grounding:xsltTransformationString>None (XSL)</grounding:xsltTransformationString>
      </grounding:WsdlInputMessageMap>
    </grounding:wsdlInput>
    <grounding:wsdlInput>
      <grounding:WsdlInputMessageMap>
        <grounding:owlsParameter rdf:resource="#_LATITUDE1"/>
        <grounding:wsdlMessagePart rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI"
        >http://127.0.0.1/wsdl/CalculateDistanceMiles#_LATITUDE1</grounding:wsdlMessagePart>
        <grounding:xsltTransformationString>None (XSL)</grounding:xsltTransformationString>
      </grounding:WsdlInputMessageMap>
    </grounding:wsdlInput>
    <grounding:owlsProcess rdf:resource="#CALCULATE_DISTANCE_MILES_PROCESS"/>
    <grounding:wsdlOutput>
      <grounding:WsdlOutputMessageMap>
        <grounding:owlsParameter rdf:resource="#_DISTANCE"/>
        <grounding:wsdlMessagePart rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI"
        >http://127.0.0.1/wsdl/CalculateDistanceMiles#_DISTANCE</grounding:wsdlMessagePart>
        <grounding:xsltTransformationString>None (XSL)</grounding:xsltTransformationString>
      </grounding:WsdlOutputMessageMap>
    </grounding:wsdlOutput>
    <grounding:wsdlOperation>
      <grounding:WsdlOperationRef>
        <grounding:operation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI"
        >http://127.0.0.1/wsdl/CalculateDistanceMiles#get_DISTANCE</grounding:operation>
        <grounding:portType rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI"
        >http://127.0.0.1/wsdl/CalculateDistanceMiles#CalculateDistanceMilesSoap</grounding:portType>
      </grounding:WsdlOperationRef>
    </grounding:wsdlOperation>
    <grounding:wsdlInput>
      <grounding:WsdlInputMessageMap>
        <grounding:owlsParameter rdf:resource="#_LATITUDE2"/>
        <grounding:wsdlMessagePart rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI"
        >http://127.0.0.1/wsdl/CalculateDistanceMiles#_LATITUDE2</grounding:wsdlMessagePart>
        <grounding:xsltTransformationString>None (XSL)</grounding:xsltTransformationString>
      </grounding:WsdlInputMessageMap>
    </grounding:wsdlInput>
    <grounding:wsdlDocument rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI"
    >http://127.0.0.1/wsdl/calculateDistanceInMiles.wsdl</grounding:wsdlDocument>
    <grounding:wsdlInputMessage rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI"
    >http://127.0.0.1/wsdl/CalculateDistanceMiles#get_DISTANCERequest</grounding:wsdlInputMessage>
    <grounding:wsdlInput>
      <grounding:WsdlInputMessageMap>
        <grounding:owlsParameter rdf:resource="#_LONGITUDE2"/>
        <grounding:wsdlMessagePart rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI"
        >http://127.0.0.1/wsdl/CalculateDistanceMiles#_LONGITUDE2</grounding:wsdlMessagePart>
        <grounding:xsltTransformationString>None (XSL)</grounding:xsltTransformationString>
      </grounding:WsdlInputMessageMap>
    </grounding:wsdlInput>
    <grounding:wsdlOutputMessage rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI"
    >http://127.0.0.1/wsdl/CalculateDistanceMiles#get_DISTANCEResponse</grounding:wsdlOutputMessage>
  </grounding:WsdlAtomicProcessGrounding>

</rdf:RDF>
