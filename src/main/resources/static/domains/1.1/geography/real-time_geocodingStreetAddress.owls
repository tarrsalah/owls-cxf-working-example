<?xml version="1.0" encoding="WINDOWS-1252"?>
<rdf:RDF  xmlns:owl       = "http://www.w3.org/2002/07/owl#"
xmlns:rdfs      = "http://www.w3.org/2000/01/rdf-schema#"
xmlns:rdf       = "http://www.w3.org/1999/02/22-rdf-syntax-ns#"
xmlns:service   = "http://www.daml.org/services/owl-s/1.1/Service.owl#"
xmlns:process   = "http://www.daml.org/services/owl-s/1.1/Process.owl#"
xmlns:profile    = "http://www.daml.org/services/owl-s/1.1/Profile.owl#"
xmlns:grounding = "http://www.daml.org/services/owl-s/1.1/Grounding.owl#"
xmlns:expr      = "http://www.daml.org/services/owl-s/1.1/generic/Expression.owl#"
xmlns:pddlexpr  = "http://127.0.0.1/ontology/PDDLExpression.owl#"

xml:base        = "http://127.0.0.1/services/1.1/real-time_geocodingStreetAddress.owls">

<owl:Ontology rdf:about="">
<owl:imports rdf:resource="http://127.0.0.1/ontology/Service.owl" />
<owl:imports rdf:resource="http://127.0.0.1/ontology/Process.owl" />
<owl:imports rdf:resource="http://127.0.0.1/ontology/Profile.owl" />
<owl:imports rdf:resource="http://127.0.0.1/ontology/Grounding.owl" />
<owl:imports rdf:resource="http://127.0.0.1/ontology/Expression.owl" />
<owl:imports rdf:resource="http://127.0.0.1/ontology/PDDLExpression.owl" />
<owl:imports rdf:resource="http://127.0.0.1/ontology/protonu.owl" />
<owl:imports rdf:resource="http://127.0.0.1/ontology/protont.owl" />
<owl:imports rdf:resource="http://127.0.0.1/ontology/core-plus-office.owl" />
<owl:imports rdf:resource="http://127.0.0.1/ontology/geographydataset.owl" />
</owl:Ontology>

<service:Service rdf:ID="REALTIME_GEOCODING_ADDRESS_SERVICE">
<service:presents rdf:resource="#REALTIME_GEOCODING_ADDRESS_PROFILE"/>
<service:describedBy rdf:resource="#REALTIME_GEOCODING_ADDRESS_PROCESS"/>
<service:supports rdf:resource="#REALTIME_GEOCODING_ADDRESS_GROUNDING"/>
</service:Service>

<profile:Profile rdf:ID="REALTIME_GEOCODING_ADDRESS_PROFILE">
<service:isPresentedBy rdf:resource="#REALTIME_GEOCODING_ADDRESS_SERVICE"/>
<profile:serviceName xml:lang="en">
Real-time geocoding service.
</profile:serviceName>
<profile:textDescription xml:lang="en">
The real-time geocoding service is a web based geocoding service providing real-time geocoding of addresses from 26 countries and areas. It returns the best mathching geographic location and actual matching address.
Valid UserID should be provided in order to use this service.
</profile:textDescription>
<profile:hasInput  rdf:resource="#_USERID"/>
<profile:hasInput  rdf:resource="#_CITY"/>
<profile:hasInput  rdf:resource="#_STATE"/>
<profile:hasInput  rdf:resource="#_COUNTRY"/>
<profile:hasInput  rdf:resource="#_ADDRESS"/>
<profile:hasInput  rdf:resource="#_ZIPCODE"/>
<profile:hasOutput rdf:resource="#_LOCATION"/>
<profile:hasOutput  rdf:resource="#_MATCHING-ADDRESS"/>

<profile:has_process rdf:resource="REALTIME_GEOCODING_ADDRESS_PROCESS" /></profile:Profile>

<process:AtomicProcess rdf:ID="REALTIME_GEOCODING_ADDRESS_PROCESS">
<service:describes rdf:resource="#REALTIME_GEOCODING_ADDRESS_SERVICE"/>
<process:hasInput  rdf:resource="#_USERID"/>
<process:hasInput  rdf:resource="#_CITY"/>
<process:hasInput  rdf:resource="#_STATE"/>
<process:hasInput  rdf:resource="#_COUNTRY"/>
<process:hasInput  rdf:resource="#_ADDRESS"/>
<process:hasInput  rdf:resource="#_ZIPCODE"/>
<process:hasOutput rdf:resource="#_LOCATION"/>
<process:hasOutput  rdf:resource="#_MATCHING-ADDRESS"/>
<process:hasPrecondition>
          <pddlexpr:PDDL-Expression>
            <expr:expressionBody rdf:datatype="http://www.w3.org/2001/XMLSchema#string">
			     (and (http://127.0.0.1/ontology/protont.owl#locatedIn ?http://127.0.0.1/services/1.1/real-time_geocodingStreetAddress.owls#_CITY ?http://127.0.0.1/services/1.1/real-time_geocodingStreetAddress.owls#_STATE)
				      (http://127.0.0.1/ontology/protont.owl#locatedIn ?http://127.0.0.1/services/1.1/real-time_geocodingStreetAddress.owls#_STATE ?http://127.0.0.1/services/1.1/real-time_geocodingStreetAddress.owls#_COUNTRY)
					  (http://127.0.0.1/ontology/core-plus-office.owl#Authorized ?http://127.0.0.1/services/1.1/real-time_geocodingStreetAddress.owls#_USERID))
			</expr:expressionBody>
          </pddlexpr:PDDL-Expression>
</process:hasPrecondition>
</process:AtomicProcess>

<process:Input rdf:ID="_USERID">
<process:parameterType rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">http://127.0.0.1/ontology/geographydataset.owl#UniqueIdentifier</process:parameterType>
<rdfs:label></rdfs:label>
</process:Input>

<process:Input rdf:ID="_CITY">
<process:parameterType rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">http://127.0.0.1/ontology/protonu.owl#City</process:parameterType>
<rdfs:label></rdfs:label>
</process:Input>

<process:Input rdf:ID="_STATE">
<process:parameterType rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">http://127.0.0.1/ontology/protonu.owl#State</process:parameterType>
<rdfs:label></rdfs:label>
</process:Input>

<process:Input rdf:ID="_COUNTRY">
<process:parameterType rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">http://127.0.0.1/ontology/protonu.owl#Country</process:parameterType>
<rdfs:label></rdfs:label>
</process:Input>

<process:Input rdf:ID="_ADDRESS">
<process:parameterType rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">http://127.0.0.1/ontology/protonu.owl#Address</process:parameterType>
<rdfs:label></rdfs:label>
</process:Input>

<process:Input rdf:ID="_ZIPCODE">
<process:parameterType rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">http://127.0.0.1/ontology/geographydataset.owl#US5DigitZipCode</process:parameterType>
<rdfs:label></rdfs:label>
</process:Input>

<process:Output  rdf:ID="_LOCATION">
<process:parameterType rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">http://127.0.0.1/ontology/protont.owl#Location</process:parameterType>
<rdfs:label></rdfs:label>
</process:Output>

<process:Output rdf:ID="_MATCHING-ADDRESS">
<process:parameterType rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">http://127.0.0.1/ontology/protonu.owl#Address</process:parameterType>
<rdfs:label></rdfs:label>
</process:Output>

<grounding:WsdlGrounding rdf:ID="REALTIME_GEOCODING_ADDRESS_GROUNDING">
  <service:supportedBy rdf:resource="#REALTIME_GEOCODING_ADDRESS_SERVICE"/>
    <grounding:hasAtomicProcessGrounding>
      <grounding:WsdlAtomicProcessGrounding rdf:ID="REALTIME_GEOCODING_ADDRESS_AtomicProcessGrounding"/>
    </grounding:hasAtomicProcessGrounding>
</grounding:WsdlGrounding>

<grounding:WsdlAtomicProcessGrounding rdf:about="#REALTIME_GEOCODING_ADDRESS_AtomicProcessGrounding">
    <grounding:wsdlInputMessage rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI"
    >http://127.0.0.1/wsdl/RealtimeGeocodingAddress#get_LOCATION_MATCHING-ADDRESSRequest</grounding:wsdlInputMessage>
    <grounding:wsdlInput>
      <grounding:WsdlInputMessageMap>
        <grounding:owlsParameter rdf:resource="#_COUNTRY"/>
        <grounding:wsdlMessagePart rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI"
        >http://127.0.0.1/wsdl/RealtimeGeocodingAddress#_COUNTRY</grounding:wsdlMessagePart>
        <grounding:xsltTransformationString>None (XSL)</grounding:xsltTransformationString>
      </grounding:WsdlInputMessageMap>
    </grounding:wsdlInput>
    <grounding:wsdlInput>
      <grounding:WsdlInputMessageMap>
        <grounding:owlsParameter rdf:resource="#_CITY"/>
        <grounding:wsdlMessagePart rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI"
        >http://127.0.0.1/wsdl/RealtimeGeocodingAddress#_CITY</grounding:wsdlMessagePart>
        <grounding:xsltTransformationString>None (XSL)</grounding:xsltTransformationString>
      </grounding:WsdlInputMessageMap>
    </grounding:wsdlInput>
    <grounding:wsdlOutput>
      <grounding:WsdlOutputMessageMap>
        <grounding:owlsParameter rdf:resource="#_LOCATION"/>
        <grounding:wsdlMessagePart rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI"
        >http://127.0.0.1/wsdl/RealtimeGeocodingAddress#_LOCATION</grounding:wsdlMessagePart>
        <grounding:xsltTransformationString>None (XSL)</grounding:xsltTransformationString>
      </grounding:WsdlOutputMessageMap>
    </grounding:wsdlOutput>
    <grounding:owlsProcess rdf:resource="#REALTIME_GEOCODING_ADDRESS_PROCESS"/>
    <grounding:wsdlInput>
      <grounding:WsdlInputMessageMap>
        <grounding:owlsParameter rdf:resource="#_ADDRESS"/>
        <grounding:wsdlMessagePart rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI"
        >http://127.0.0.1/wsdl/RealtimeGeocodingAddress#_ADDRESS</grounding:wsdlMessagePart>
        <grounding:xsltTransformationString>None (XSL)</grounding:xsltTransformationString>
      </grounding:WsdlInputMessageMap>
    </grounding:wsdlInput>
    <grounding:wsdlOperation>
      <grounding:WsdlOperationRef>
        <grounding:operation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI"
        >http://127.0.0.1/wsdl/RealtimeGeocodingAddress#get_LOCATION_MATCHING-ADDRESS</grounding:operation>
        <grounding:portType rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI"
        >http://127.0.0.1/wsdl/RealtimeGeocodingAddress#RealtimeGeocodingAddressSoap</grounding:portType>
      </grounding:WsdlOperationRef>
    </grounding:wsdlOperation>
    <grounding:wsdlInput>
      <grounding:WsdlInputMessageMap>
        <grounding:owlsParameter rdf:resource="#_USERID"/>
        <grounding:wsdlMessagePart rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI"
        >http://127.0.0.1/wsdl/RealtimeGeocodingAddress#_USERID</grounding:wsdlMessagePart>
        <grounding:xsltTransformationString>None (XSL)</grounding:xsltTransformationString>
      </grounding:WsdlInputMessageMap>
    </grounding:wsdlInput>
    <grounding:wsdlInput>
      <grounding:WsdlInputMessageMap>
        <grounding:owlsParameter rdf:resource="#_ZIPCODE"/>
        <grounding:wsdlMessagePart rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI"
        >http://127.0.0.1/wsdl/RealtimeGeocodingAddress#_ZIPCODE</grounding:wsdlMessagePart>
        <grounding:xsltTransformationString>None (XSL)</grounding:xsltTransformationString>
      </grounding:WsdlInputMessageMap>
    </grounding:wsdlInput>
    <grounding:wsdlDocument rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI"
    >http://127.0.0.1/wsdl/real-time_geocodingStreetAddress.wsdl</grounding:wsdlDocument>
    <grounding:wsdlOutputMessage rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI"
    >http://127.0.0.1/wsdl/RealtimeGeocodingAddress#get_LOCATION_MATCHING-ADDRESSResponse</grounding:wsdlOutputMessage>
    <grounding:wsdlInput>
      <grounding:WsdlInputMessageMap>
        <grounding:owlsParameter rdf:resource="#_STATE"/>
        <grounding:wsdlMessagePart rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI"
        >http://127.0.0.1/wsdl/RealtimeGeocodingAddress#_STATE</grounding:wsdlMessagePart>
        <grounding:xsltTransformationString>None (XSL)</grounding:xsltTransformationString>
      </grounding:WsdlInputMessageMap>
    </grounding:wsdlInput>
    <grounding:wsdlOutput>
      <grounding:WsdlOutputMessageMap>
        <grounding:owlsParameter rdf:resource="#_MATCHING-ADDRESS"/>
        <grounding:wsdlMessagePart rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI"
        >http://127.0.0.1/wsdl/RealtimeGeocodingAddress#_MATCHING-ADDRESS</grounding:wsdlMessagePart>
        <grounding:xsltTransformationString>None (XSL)</grounding:xsltTransformationString>
      </grounding:WsdlOutputMessageMap>
    </grounding:wsdlOutput>
  </grounding:WsdlAtomicProcessGrounding>
</rdf:RDF>
